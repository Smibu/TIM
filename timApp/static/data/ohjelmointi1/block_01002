``` {.esimerkki-western lang="zxx" xml:lang="zxx"}
public class DeMorgansLaws
{
    /// <summary>
    /// Testiohjelma DeMorganin laeille
    /// </summary>
    public static void Main()
    {
        bool p = true;
        bool q = true;
        Console.WriteLine(!(p || q) == (!p && !q));
        Console.WriteLine(!(p && q) == (!p || !q));
    }
}
```