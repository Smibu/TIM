{
  "compilerOptions": {
    "module": "system",
    "strict": true,
    "allowUnreachableCode": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "es7",
      "scripthost",
      "dom.iterable"
    ],
    "noEmit": false,
    "paths": {
      "angular": [
        "./node_modules/@types/angular"
      ],
      "jquery": [
        "./node_modules/@types/jquery"
      ],
      "io-ts": [
        "./node_modules/io-ts/lib"
      ],
      "io-ts/*": [
        "./node_modules/io-ts/*"
      ],
      "mobile-drag-drop": [
        "./node_modules/mobile-drag-drop"
      ],
      "mobile-drag-drop/*": [
        "./node_modules/mobile-drag-drop/*"
      ],
      "angular-drag-and-drop-lists": [
        "./node_modules/angular-drag-and-drop-lists"
      ],
      "angular-drag-and-drop-lists/*": [
        "./node_modules/angular-drag-and-drop-lists/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./tim/util/types",
      "./tim/util"
    ],
    "plugins": [
      {
        "name": "typescript-tslint-plugin",
        "alwaysShowRuleFailuresAsWarnings": false
      }
    ]
  },
  "exclude": [
    "node_modules",
    "jspm_packages"
  ]
}
