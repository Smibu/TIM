{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "allowUnreachableCode": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "build",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "tim/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "bower_components",
    "build-optimized",
    "build"
  ]
}
