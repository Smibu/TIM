{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "allowUnreachableCode": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "build-ide",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
    },
    "target": "es5",
    "lib": [
      "dom",
      "es7",
      "scripthost"
    ],
    "noEmit": false,
    "declarationDir": "decls",
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "jspm_packages",
    "bower_components",
    "build",
    "build-ide",
    "decls"
  ]
}
