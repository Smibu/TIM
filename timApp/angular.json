{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tim": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": ".",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "static/scripts/build",
            "index": "static/scripts/tim/index.html",
            "main": "static/scripts/tim/main.ts",
            "polyfills": "static/scripts/tim/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/bootstrap-sass/assets/fonts/bootstrap",
                "output": "/bootstrap-fonts/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/reveal.js/plugin/notes",
                "output": "/reveal/notes/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/reveal.js/plugin/zoom-js",
                "output": "/reveal/zoom-js/"
              }
            ],
            "styles": [
              "node_modules/angular-ui-grid/ui-grid.min.css",
              "node_modules/angularjs-color-picker/dist/angularjs-color-picker.min.css",
              "node_modules/angularjs-color-picker/dist/themes/angularjs-color-picker-bootstrap.min.css",
              "node_modules/angular-bootstrap-colorpicker/css/colorpicker.min.css",
              "node_modules/katex/dist/katex.min.css",
              "node_modules/mobile-drag-drop/default.css"
            ],
            "scripts": [
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "static/scripts/tim/environments/environment.ts",
                  "with": "static/scripts/tim/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tim:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tim:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tim:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "static/scripts/tim/test.ts",
            "polyfills": "static/scripts/tim/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "static/scripts/tim/favicon.ico",
              "static/scripts/tim/assets"
            ],
            "styles": [
              "static/scripts/tim/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tim:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tim:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "tim"
}
