rm -rf Run
mkdir Run
ln -s /cs/java/quarum/Library Library 2>/dev/null
ln -s /cs/java/quarum/Quorum.jar Quorum.jar 2>/dev/null
ln -s /cs/java/quarum/QuorumStandardLibrary.jar QuorumStandardLibrary.jar 2>/dev/null 
ln -s /cs/java/quarum/QuorumStandardPlugins.jar QuorumStandardPlugins.jar 2>/dev/null
ln -s /cs/java/quarum/Run/Graphics Run/Graphics 2>/dev/null
ln -s /cs/java/quarum/Run/jni Run/jni 2>/dev/null 
ln -s /cs/java/quarum/Run/Robots Run/Robots 2>/dev/null 
java -jar /cs/java/quarum/Quorum.jar hello.q 
cp Run/Default.jar .
java -jar Default.jar

