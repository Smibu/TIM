{
  "name": "jsrunner",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npx ts-node -r tsconfig-paths/register ./bin/www.ts",
    "buildtools": "npx rollup -c",
    "buildserver": "npx rollup -c rollup.server.config.js",
    "postinstall": "npm run buildtools",
    "lint": "eslint --report-unused-disable-directives --format codeframe --ext ts .",
    "lintfix": "eslint --format codeframe --ext ts . --fix"
  },
  "dependencies": {
    "acorn": "^7.3.1",
    "cookie-parser": "~1.4.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "fp-ts": "^2.6.6",
    "http-errors": "~1.7.3",
    "io-ts": "^2.2.6",
    "isolated-vm": "^3.3.3",
    "jade": "~1.11.0",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.6.3",
    "@types/morgan": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "rollup": "^2.18.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
